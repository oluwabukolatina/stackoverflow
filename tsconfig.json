{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
     "sourceMap": true,
    "outDir": "build",
     "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true /* Disallow inconsistently-cased references to the same file. */
  },
  "exclude": [
    "**/*.test.ts"
  ]
}
